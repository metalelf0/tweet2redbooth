<%# add navigation links to this file %>
<% if user_signed_in? %>
  <li><%= link_to 'Track tweets', url_for(controller:'twitter', action: 'index')%></li>
  <li><%= link_to 'Edit account', edit_user_registration_path %></li>
  <li><%= link_to 'Sign out', destroy_user_session_path, :method=>'delete' %></li>
<% else %>
  <li><%= link_to 'Home', root_path, class: 'navbar-brand' %></li>
  <li><%= link_to "Sign in with Redbooth", user_omniauth_authorize_path(:redbooth) %></li>
  <li><%= link_to "Cancel registration", cancel_user_registration_path %></li>
<% end %>
